[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "dashPivottable"]
	path = dashPivottable
	url = https://github.com/plotly/dash-pivottable
	shallow = true
	registered = true
[submodule "plotly"]
	path = plotly
	url = https://github.com/plotly/plotly.r
	shallow = true
	registered = true
[submodule "ggplot2"]
	path = ggplot2
	url = https://github.com/tidyverse/ggplot2
	shallow = true
	registered = false
